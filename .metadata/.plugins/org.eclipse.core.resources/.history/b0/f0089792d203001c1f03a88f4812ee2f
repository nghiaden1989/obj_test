#include "exam.h"
void Say(const char* message)
{
	printf(message);
}
void securityExample::test(void)
{
	getTime();
	//printf("%s",timeBuff);
	convert();
}
void securityExample::getTime()
{
	time_t my_time = time(NULL);
	timeBuff = ctime(&my_time);
}
void securityExample::convert()
{
	char *token = strtok(timeBuff," ");
	while (token != NULL)
	    {
	        printf("%s\n", token);
	        token = strtok(NULL, " ");
	    }
}


// helper

bool securityExample::strCompare(char* str1, char* str2, uint8_t length)
{
	for(int i = 0; i< length; i++)
	{
		if(str1[i] != str2[i])
			return false;
	}
	return true;
}

