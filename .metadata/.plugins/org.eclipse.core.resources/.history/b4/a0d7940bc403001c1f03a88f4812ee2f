#include <iostream>
#include "exam.h"
#include <time.h>
#include <stdlib.h>

#include <chrono>
#include <ctime>
using namespace std;

int main() {

	time_t my_time = time(NULL);

	    // ctime() used to give the present time
	    printf("%s", ctime(&my_time));
	    return 0;
	return 0;
}
