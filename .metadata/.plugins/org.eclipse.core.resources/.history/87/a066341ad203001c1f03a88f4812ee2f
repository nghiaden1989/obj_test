#include "exam.h"
void Say(const char* message)
{
	printf(message);
}
void securityExample::test(void)
{
	getTime();
	//printf("%s",timeBuff);
	convert();
}
void securityExample::getTime()
{
	time_t my_time = time(NULL);
	timeBuff = ctime(&my_time);
}
void securityExample::convert()
{
	//char str[] = "Mon Aug 23 10:40:38 2021";
	char *token = strtok(timeBuff," ");
	while (token != NULL)
	    {
	        printf("%s\n", token);
	        token = strtok(NULL, " ");
	    }
}

